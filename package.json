{
  "name": "@dugjason/front-node",
  "version": "0.1.0-beta.1",
  "description": "Modern TypeScript SDK for the Front.com API",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "lint": "biome check .",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "generate:types": "openapi-ts"
  },
  "keywords": [
    "front",
    "api",
    "sdk",
    "typescript",
    "nodejs"
  ],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.12.4",
  "devDependencies": {
    "@biomejs/biome": "2.2.2",
    "@faker-js/faker": "10.0.0",
    "@hey-api/openapi-ts": "0.82.3",
    "@types/node": "^20.19.1",
    "msw": "^2.11.1",
    "msw-auto-mock": "0.31.0",
    "typescript": "^5.9.2",
    "vitest": "3.2.4"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist"
  ]
}
